[auth.keystone]
enabled = true
auth_url = {{ grafana_keystone_url }}
[database]
type = mysql
host = {{ grafana_mysql_host }}
name = {{ grafana_mysql_db }}
user = {{ grafana_mysql_user }}
password = {{ grafana_mysql_password }}
ssl_mode = skip-verify
[security]
admin_user = admin
admin_password = {{ grafana_admin_password }}
